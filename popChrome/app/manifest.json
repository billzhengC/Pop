{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon.png",
    "128": "images/icon.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon.png",
      "38": "images/icon.png"
    },
    "default_title": "POP",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "omnibox": {
    "keyword": "POP"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/jquery-2.1.3.min.js",
        "scripts/semantic.min.js",
        "scripts/CommentCoreLibrary.js",
        "scripts/contentscript.js"
      ],
      "css": [
        "styles/semantic.min.css",
        "styles/style.css",
        "styles/my-page-styles.css",
        "styles/main.css"],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "history",
    "management",
    "http://*/*",
    "https://*/*"
  ]
}
